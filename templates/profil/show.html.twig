{% extends 'base.html.twig' %}

{% block title %}Profil{% endblock %}

{% block body %}
    <h1>Profil</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ profil.id }}</td>
            </tr>
            <tr>
                <th>Firstname</th>
                <td>{{ profil.firstname }}</td>
            </tr>
            <tr>
                <th>Lastname</th>
                <td>{{ profil.lastname }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ profil.description }}</td>
            </tr>
            <tr>
                <th>Age</th>
                <td>{{ profil.age }}</td>
            </tr>
            <tr>
                <th>City</th>
                <td>{{ profil.city }}</td>
            </tr>
            <tr>
                <th>Mail</th>
                <td>{{ profil.mail }}</td>
            </tr>
            <tr>
                <th>Phone</th>
                <td>{{ profil.phone }}</td>
            </tr>
            <tr>
                <th>Job</th>
                <td>{{ profil.job }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_profil_index') }}">back to list</a>

    <a href="{{ path('app_profil_edit', {'id': profil.id}) }}">edit</a>

    {{ include('profil/_delete_form.html.twig') }}
{% endblock %}
